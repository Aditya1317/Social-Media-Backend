<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="210320250424-1" author="aditya">

        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="reels"/>
            </not>
        </preConditions>

        <createTable tableName="reels">
            <column name="r_id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="content_desc" type="VARCHAR(255)"/>
            <column name="tags" type="VARCHAR(255)"/>
            <column name="date" type="TIMESTAMP" defaultValueComputed="CURRENT_TIMESTAMP"/>
        </createTable>

    </changeSet>





</databaseChangeLog>
